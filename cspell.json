  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "words": [
    "Aknowlegements",
    "almalinux",
    "Alpine",
    "Antoine",
    "antoinemartin",
    "APPLOCALDATA",
    "ARCHITEW",
    "archlinux",
    "Archlinux",
    "bsdtar",
    "buildah",
    "buildctl",
    "chroot",
    "debian",
    "Debian",
    "distro",
    "distros",
    "doas",
    "elif",
    "github",
    "gitstatus",
    "HKCU",
    "Incus",
    "jekyll",
    "kaweezle",
    "keyserver",
    "LASTEXITCODE",
    "minirootfs",
    "miniwsl",
    "Netsdk",
    "Nuget",
    "NuGet",
    "Nupkg",
    "Openance",
    "opencontainers",
    "openpgp",
    "openrc",
    "opensuse",
    "OpenSUSE",
    "openwrt",
    "p10k",
    "pipx",
    "powerlevel",
    "PowerShellGet",
    "PSGallery",
    "Referer",
    "rockylinux",
    "rootfs",
    "RootFS",
    "skopeo",
    "Tumbleweed",
    "Ubuntu",
    "ulyana",
    "ulyssa",
    "umoci",
