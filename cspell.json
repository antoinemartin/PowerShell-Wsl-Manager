    "updatebot",
    "vhdx",
    "voidlinux",
    "Wsl",
    "WSL",
    "yubikey",
    "Zsh"
  ],
  "ignoreWords": [
    "bsdtar",
    "rootfs",
    "archlinuxbase",
    "origfs",
    "xpkf",
    "zcpf",
    "zxpkf",
    "resolv",
    "userspec",
    "linux",
    "esac",
    "buildx",
    "softprops",
    "badlinux"
  ],
  "ignorePaths": [
    "**/*.svg",
    "**/*.ico",
    "**/*.zip",
    "**/*.gz",
    "**/*.xz",
    "**/*.7z",
    "**/*.tar",
    "**/*.nupkg",
    "docs/assets/**",
    ".git/**",
    "node_modules/**"
  ],
  "dictionaries": [
    "powershell",
    "softwareTerms"
  ],
  "overrides": [
    {
      "filename": "**/*.ps1",
      "languageId": "powershell"
    },
    {
      "filename": "**/*.psm1",
      "languageId": "powershell"
    },
    {
      "filename": "**/*.psd1",
      "languageId": "powershell"
    },
    {
      "filename": "**/*.md",
      "languageId": "markdown"
    },
    {
      "filename": "**/.github/workflows/*.yml",
      "languageId": "yaml"
    },
    {
      "filename": "**/.github/workflows/*.yaml",
      "languageId": "yaml"
    }
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "/^\\s*```[\\s\\S]*?^\\s*```/gm"
      ]
    }
  ]
}
